{
    "name": "synergitech/laravel-sweetalert",
    "description": "A PHP package to show Sweet Alerts with the Laravel Framework",
    "keywords": ["laravel", "alert", "sweet", "notifier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Uziel Bueno",
            "email": "ux.webs@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "~5.0",
        "illuminate/session": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "SynergiTech\\SweetAlert\\": "src/SweetAlert/"
        },
        "files": [
          "src/SweetAlert/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "SynergiTech\\SweetAlert\\SweetAlertServiceProvider"
            ],
            "aliases": {
                "Alert": "SynergiTech\\SweetAlert\\SweetAlert"
            }
        }
    }
}
